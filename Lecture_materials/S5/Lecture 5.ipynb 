   "source": [
    "# a quick Ber(p) random variable generator\n",
    "def randber(shape, p=0.5):\n",
    "    u = np.random.rand(*shape)\n",
    "    u[u < 1 - p] = 0\n",
    "    u[u >= 1 - p] = 1\n",
    "\n",
    "    return np.int64(u)\n",
    "\n",
    "\n",
    "n = 100\n",
    "p = 0.2\n",
    "handedness = randber((n,), p)\n",
    "sns.histplot(handedness)\n",
    "\n",
    "print(handedness / n)\n",
    "print((handedness / n).sum())"
   ]
  },
